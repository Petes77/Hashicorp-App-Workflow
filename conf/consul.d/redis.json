{
  "service": {
    "name": "redis",
    "tags": ["primary"],
    "address": "",
    "meta": {
      "meta": "The Redis Service"
    },
    "port": 6379,
    "enable_tag_override": false,
    "checks": [
      {
        "args": ["/usr/local/bootstrap/scripts/consul_redis_ping.sh"],
        "interval": "10s"
      },
      {
          "args": ["/usr/local/bootstrap/scripts/consul_redis_verify.sh"],
          "interval": "10s"
      }
    ]
  }
}